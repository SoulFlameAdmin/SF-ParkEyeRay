# Rename this file to .env and fill values.
PORT=4000
# Frontend origin allowed for CORS (change to your domain in production)
ORIGIN=http://localhost:8000

# Twilio credentials
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# Your Twilio phone number (or approved sender ID, depending on country)
TWILIO_FROM=+1XXXXXXXXXX

# Used when user enters local numbers like 088...
DEFAULT_COUNTRY_CODE=359
